{
  "manifest_version": 2,
  "name": "Archive.ph Link Opener",
  "version": "1.0.0",
  "description": "Right-click on links to open them via archive.ph",
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "128": "icons/avatar_90745502f381_128.pnj"
  },
  "browser_specific_settings": {
    "gecko": {
    "data_collection_permissions": {
      "required": ["none"]
    }
  }
}
}

